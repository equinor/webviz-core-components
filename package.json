{
    "name": "webviz_core_components",
    "version": "0.0.1",
    "description": "Core components for webviz config",
    "repository": {
        "type": "git",
        "url": "git://github.com/equinor/webviz-core-components.git"
    },
    "bugs": {
        "url": "https://github.com/equinor/webviz-core-components/issues"
    },
    "homepage": "https://github.com/equinor/webviz-core-components",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --devtool inline-source-map --entry ./src/demo/index.tsx --open",
        "postinstall": "cp ./package.json webviz_core_components/package-info.json",
        "build:js": "webpack --mode production",
        "build:js-dev": "webpack --mode development",
        "build:py_and_r": "dash-generate-components ./src/lib/components webviz_core_components -p package-info.json --r-prefix '' --ignore .*.js$",
        "build": "npm run build:js && npm run build:js-dev && npm run build:py_and_r",
        "typecheck": "tsc --noEmit",
        "format": "eslint --fix 'src/**/*.+(ts|tsx|js|jsx|json|css)'",
        "linting": "eslint 'src/**/*.+(ts|tsx|js|jsx|json|css)'",
        "validate": "npm run typecheck && npm run linting"
    },
    "author": "R&T Equinor <akia@equinor.com>",
    "license": "MIT",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "html2canvas": "^1.0.0-rc.7",
        "ramda": "^0.27.1",
        "react-colorscales": "^0.7.3",
        "reactour": "^1.15.2",
        "source-map-loader": "^2.0.1",
        "styled-components": "^4.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@types/jest": "^26.0.20",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^2.32.0",
        "@typescript-eslint/parser": "^2.32.0",
        "ts-loader": "^8.0.17",
        "babel-loader": "^8.1.0",
        "copyfiles": "^2.0.0",
        "css-loader": "^2.1.1",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-react": "^7.19.0",
        "file-loader": "^6.2.0",
        "mini-css-extract-plugin": "^0.8.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "prettier": "^1.18.2",
        "prop-types": "^15.7.2",
        "react": "^16.9.0",
        "react-docgen": "^4.1.1",
        "react-dom": "^16.8.6",
        "source-map-loader": "^2.0.1",
        "style-loader": "^1.0.0",
        "styled-jsx": "^3.2.1",
        "typescript": "^3.8.3",
        "url-loader": "^2.1.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.11.0"
    },
    "engines": {
        "node": ">=12.14.0",
        "npm": ">=6.14.6"
    }
}
